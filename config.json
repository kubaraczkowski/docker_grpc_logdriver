{
	"description": "GRPC log driver",
	"documentation": "-",
	"entrypoint": ["/usr/bin/docker-log-driver_arm64"],
	"interface": {
		"types": ["docker.logdriver/1.0"],
		"socket": "grpc.sock"
	},
	"network": {
		"type": "host"
		},
	"env": [
		{
			"name": "LOG_LEVEL",
			"description": "Set log level to output for plugin logs",
			"value": "info",
			"settable": ["value"]
		}
	]
}


